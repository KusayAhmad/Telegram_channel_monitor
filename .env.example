# مثال على ملف الإعدادات - انسخه إلى .env وعدّل القيم

# Telegram API Credentials
# احصل عليها من https://my.telegram.org
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# معرّف المستخدم للإشعارات (استخدم @userinfobot للحصول عليه)
NOTIFY_USER_ID=your_user_id_here

# وجهة إرسال الإشعارات (اختياري)
# يمكن أن يكون:
# - username قناة أو مجموعة: @MyChannel أو @MyGroup  
# - معرّف رقمي للقناة/المجموعة: -1001234567890
# - معرّف رقمي لشخص: 123456789
# إذا تُرك فارغاً، سيتم إرسال الإشعارات إلى Saved Messages
NOTIFY_CHAT_ID=

# اسم ملف الجلسة
SESSION_NAME=my_account

# إعدادات البوت (اختياري - احصل على التوكن من @BotFather)
BOT_TOKEN=your_bot_token_here

# إعدادات Discord (اختياري)
DISCORD_WEBHOOK_URL=

# إعدادات البريد الإلكتروني (اختياري)
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=
EMAIL_PASSWORD=
EMAIL_TO=

# إعدادات لوحة التحكم
DASHBOARD_SECRET_KEY=change_this_to_a_random_secret_key
DASHBOARD_PORT=5000
DASHBOARD_HOST=127.0.0.1
